{
   "browser_action": {
      "default_icon": "images/19.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_extname__"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery-1.7.1.min.js", "js/swiper.min.js", "js/popup.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/128.png"
   },
   "manifest_version": 2,
   "name": "__MSG_extname__",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "contextMenus" ],
   "version": "1.0.1.1"
}
