{
   "name": "访问助手",
   "version": "1.0.0",
   "manifest_version": 3,
   "description": "优化体验",
   "icons": {
      "128": "images/128.png"
   },
   "action": {
      "default_icon": "images/128.png",
      "default_title": "访问助手"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery-1.7.1.min.js", "js/swiper.min.js", "js/main.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ]
}
